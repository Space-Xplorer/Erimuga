# Backend production environment for custom domain
NODE_ENV=production
PORT=10000

# Database
MONGODB_URI=your_production_mongodb_uri

# Session security
SESSION_SECRET=your_super_secure_session_secret_for_production

# Domain settings
BASE_URL=https://api.erindmuga.in
FRONTEND_URL=https://www.erindmuga.in

# Cloudinary (your existing credentials)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Google OAuth (production credentials)
GOOGLE_CLIENT_ID=your_production_google_client_id
GOOGLE_CLIENT_SECRET=your_production_google_client_secret
GOOGLE_CALLBACK_URL=https://api.erindmuga.in/user/auth/google/callback

# Razorpay (production keys)
RAZORPAY_KEY_ID=your_production_razorpay_key
RAZORPAY_KEY_SECRET=your_production_razorpay_secret